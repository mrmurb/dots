Xft.dpi: 120

URxvt.font:                     xft:Source Code Pro:regular:size=10
URxvt.scrollBar:                false
URxvt.perl-ext-common:          default,clipboard,url-select
URxvt.url-select.launcher:      /usr/bin/google-chrome-stable
URxvt.matcher.button:           1
URxvt.url-select.underline:     true
URxvt.keysym.Shift-Control-C:   perl:clipboard:copy
URxvt.keysym.Shift-Control-V:   perl:clipboard:paste
! Disable ctrl-shift keycap picture insert mode
URxvt.iso14755:                 false
URxvt.iso14755_52:              false

! rofi
rofi.font:      Source Code Pro 10
rofi.lines:     10
rofi.opacity:   80
rofi.terminal:  urxvtc

! Set color scheme
#include "dots/colorschemes/gruvbox"

